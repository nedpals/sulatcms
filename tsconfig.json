{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": false,
        "lib": ["es6", "dom"],
        "sourceMap": true,
        "module": "es6",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "target": "es5",
        "jsx": "react",
        "jsxFactory": "m",
        "allowJs": true,
        "baseUrl": "src"
    }
}